Height = float(input("Enter the height in cm: "))  
Weight = float(input("Enter the weight in kg: "))  
BMI = Weight / (Height/100)**2  
print("Body Mass Index ", BMI)  
if BMI <= 18.5:  
    print("Underweight")  
elif BMI <= 24.9:  
    print("Perfect")  
elif BMI <= 29.9:  
    print("Over Weight")  
else:  
    print("Data insufficient")  
